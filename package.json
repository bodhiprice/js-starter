{
  "name": "reactor",
  "version": "1.0.0",
  "description": "JavaScript project starter",
  "main": "index.js",
  "browserify": {
    "transform": "babelify"
  },
  "babel": {
    "presets": "es2015"
  },
  "scripts": {
    "build-js": "browserify js/main.js -o build/scripts.js",
    "clean": "rm -r build/*",
    "move": "cp index.html build",
    "build": "npm run clean | npm run build-js | npm run move | nodemon ./server.js localhost 7000"
  },
  "author": "John Hannah",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.3"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.1",
    "nodemon": "^1.8.1"
  }
}
